<?xml version='1.0' encoding='utf-8'?>
<widget id="org.mysociety.CuidoMiDN" ios-CFBundleVersion="1.0.2.20170706.1" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
